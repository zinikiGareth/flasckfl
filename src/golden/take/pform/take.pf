package test.golden
  test.golden.take
    [var] n
    [ctor] Nil
   =
    Nil
  test.golden.take
    1: 0
    [ctor] Cons
   =
    Nil
  test.golden.take
    [var] n
    [ctor] Cons
      head
        [var] a
      tail
        [var] b
   =
    Cons
      a
      take
        -
          n
          # 1
        b
