package test.golden
  test.golden.filter
    [var] f
    [ctor] Nil
   =
    Nil
  test.golden.filter
    [var] f
    [ctor] Cons
      head
        [var] a
      tail
        [var] l
   =
    if (f a)
      Cons
        a
        filter
          f
          l
    else
      filter
        f
        l
