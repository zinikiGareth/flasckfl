	test acor works
		data HasAcor ha <- HasAcor.make
		assert (ha.msg)
			Args "hello" 42
			
		assert (ha.top)
			Args "hello" 42

	test fn acor works			
		data HasAcor ha <- HasAcor.make
		assert (getTop ha)
			Args "hello" 42

	test we can use it
		data HasAcor ha <- HasAcor.make
		data Use use <- Use.doit ha
		assert (use.first)
			Args "hello" 42
