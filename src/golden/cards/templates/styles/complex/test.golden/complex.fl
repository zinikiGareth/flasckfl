	card Complex
		state
			String msg <- 'hello'
			Boolean value <- True
			String somestyle

		template message
			styleme <- msg
				| => 'notice-me'
			content
				| value <- 'greetings'
					| => 'my-style'
				<- 'salutations'
					| value => 'something-mumble'
					| => 'fine'
			andme
				| value => 'a-style' somestyle
