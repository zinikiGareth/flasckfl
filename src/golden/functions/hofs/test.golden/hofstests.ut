	test a very simple hof
		assert (doit k)
			0

	test merge Nil
		assert (merge [] "A")
			[["A"]]
	
	test merge First
		assert (merge [["A"], ["B"]] "Q")
			[["Q", "A"],["Q", "B"]]

	test merge Second
		assert (merge [["Q","A"],["Q","B"]] "R")
			[["R", "Q", "A"],["R", "Q", "B"]]
			
	test cross Nil
		assert (cross [])
			[]
	
	test cross Singleton
		assert (cross [["A","B"]])
			[["A"],["B"]]
	
	test cross One
		assert (cross [["B"], ["C","D"]])
			[["B","C"],["B","D"]]
		
	test cross Two
		assert (cross [["A","B"], ["C","D"]])
			[["A","C"],["A","D"],["B","C"],["B","D"]]
	