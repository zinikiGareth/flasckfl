	card TestCard
		state
			List data <- [ 5, 8, 2, 16 ]
			
		template
			#svg @width='400' @height='250'
				$chart
			
		d3 chart data elt
			pattern "rect"
				layout
					x = 22 * elt.idx
					y = 200 - 10 * elt.data
					fill = 'red'
					width = 20
					height = 10*elt.data
					
				enter
					<- D3Action "append" ["rect"]
					
			pattern "text"
				enter
					<- D3Action "append" ["text"]