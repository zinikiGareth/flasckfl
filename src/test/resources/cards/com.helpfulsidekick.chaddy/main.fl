	contract TabChanger
		up change (String tab)
		down changed (String tab)
	
	card Main
		state
			String cardShowing <- 'myqueues'
		
		template
			.
				> Navbar
				|| cardShowing
					| 'dashboard'
						> Dashboard
					| 'myqueues'
						> MyQueues
		
		service TabChanger changer
			change tab
				cardShowing <- tab
			
