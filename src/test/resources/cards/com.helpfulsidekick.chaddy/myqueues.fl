	card MyQueues
		
		state
			List queues <- ['Captured Items', 'TODO Today', 'This Week', 'Flasck Issues', 'Chaddy Bugs']
			List items <- ['Get Chaddy working', 'Multiple Cards', 'List Elements']
			
		template
			. : 'section' 'main'
				. : 'w-row'
					. : 'w-col' 'w-col-3'
						+ queues q : 'w-list-unstyled' 'queue-list'
							. : 'queue-item'
								q
. : 'queue-item' 'selected-queue-item'
					. : 'w-col' 'w-col-9'
						+ items i : 'w-list-unstyled'
							. : 'queue-list-item'
								i
. : 'queue-list-item'