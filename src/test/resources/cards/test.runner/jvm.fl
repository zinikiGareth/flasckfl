	contract DataStore
		up pushMy (Type t) (Any a)
	contract Echo
		up echoIt (String s)
	contract SetState
	contract MyHandler
		down missing

	entity Account
		
	card TestCard
		implements SetState
		implements Echo e
		implements DataStore ds
		
		handler MyHandler LocalHandler
			missing
				<- ds.pushMy (type Account) Account
		
		template
			"hello, world"
				click => echoHello
			
		event echoHello ev
			<- e.echoIt "hello clicked"
			
	id x = x
	x = 420

	styleIf style b
		if (b) = style
		else = ''
	